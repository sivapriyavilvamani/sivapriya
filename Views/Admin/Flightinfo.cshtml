@model IEnumerable<DhanmayaAirlines.Models.FlightInfoClass>

@{
    ViewBag.Title = "Flightinfo";
}
@{  
    //var data = (IEnumerable<DhanmayaAirlines.Models.FlightInfoClass>) ViewData["Message"];  
} 
<div>
    <table class=" table">
        <thead>
            <tr>
                @*<th>@Html.DisplayNameFor(Model => Model.FlightName)</th>
                <th>@Html.DisplayNameFor(Model => Model.AirlinesName)</th>
                <th>@Html.DisplayNameFor(Model => Model.Source)</th>
                <th>@Html.DisplayNameFor(Model => Model.Arrival)</th>*@
                <th>FlightName</th>
                <th>AirlinesName</th>
                <th>Source</th>
                <th>Arrival</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var flight in Model)
            {
                <tr>
                    <td>@flight.FlightName</td>
                    <td>@flight.AirlinesName</td>
                    <td>@flight.Source</td>
                    <td>@flight.Arrival</td>

                </tr>
            }
        </tbody>
    </table>
</div>


